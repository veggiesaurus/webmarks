extends layout

block headerScripts

block content
	.navbar.navbar-inverse(role = 'navigation')
		.container
			.navbar-header
				.navbar-brand
					span.glyphicon.glyphicon-list-alt
					|UCT Physics Webmarks
	.container
		.row
			.col-xs-12
				.alert.alert-info
					span
						|There was a problem accessing the database. Please try later. If the problem persists, contact your course convenor
			h3 Error info
			a(align="center")
				p #{errorInfo}
				p Course code: #{urlCourseCode}
				p Student ID: #{studentID}
